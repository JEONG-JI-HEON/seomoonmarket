<title>서문시장 현장스케치</title>
<div class="sub_visual">
  <div class="main_slider">
    <div class="subVisual_layout">
      <div class="layout_txt">
        <div class="moon"></div>
        <div class="sec1_title">서문시장</div>
      </div>
      <div class="layout_location">
        <div class="location_wrap">
          <a href="/" class="location_home">
            <span class="home_icon"></span>
          </a>
          <a href="/noticeMain">커뮤니티</a>
          <a href="/sketchMain">현장스케치</a>
        </div>
      </div>
    </div>
  </div>
</div>
<section class="sub_section">
  <div class="section_inner">
    <div class="section_con">
      <div class="section_title">
        <h2>현장스케치</h2>
      </div>
      <div class="notice_writeWrap">
        <form id="noticeWrite" method="post" action="/sketchUpdateInfo" enctype="multipart/form-data">
          <input type="hidden" name="id" value="<%= row.id %>">
          <div class="boardInfo">
            <input type="file" name="ske_img" value="<%= row.ske_img %>">
            <input type="text" name="write_user" placeholder="ID" value="<%= row.write_user %>">
            <input type="password" name="user_pw" placeholder="PW">
          </div>
          <div class="boardTitle">
            <input type="text" name="ske_tit" id="board_tit" placeholder="제목" value="<%= row.ske_tit %>">
          </div>
          <div class="boardCon">
            <textarea name="ske_content" id="" cols="30" rows="20" placeholder="내용"><%= row.ske_content %></textarea>
          </div>
          <div class="boardBtn">
            <input type="submit" value="등록">
            <input type="button" value="취소" onclick="javascript:location.href='/sketchMain'">
          </div>
        </form>
      </div>
    </div>
  </div>
</section>